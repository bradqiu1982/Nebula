<!DOCTYPE html>
<html>

<head>
    <meta http-equiv="X-UA-Compatible" content="IE=8,IE=9,IE=10" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="@Url.Content("~/Content/themes/base/jquery-ui.css")" rel="stylesheet" type="text/css" />
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/jquery-ui")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/datatables")
    <link href="@Url.Content("~/Content/themes/bootstrapfileinput.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/themes/jquery.dataTables.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/Nebula.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Scripts/lobi/lobiadmin-with-plugins.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Scripts/lobi/font-awesome.min.css")" rel="stylesheet" type="text/css" />

    <script src="~/Scripts/jquery.stickytableheaders.js"></script>
    <script src="~/Scripts/ajaxfileupload.js" type="text/javascript"></script>
    <script src="~/Scripts/jquery.base64.js" type="text/javascript"></script>
    <script src="../../ckeditor/ckeditor.js"></script>

    <style type="text/css">
        .tableFloatingHeaderOriginal th {
            background-color: #006DC0;
            font-family: 'Arial Narrow';
            font-size: 15px;
        }

        .modal {
            text-align: center;
            padding: 0 !important;
        }

            .modal:before {
                content: '';
                display: inline-block;
                height: 100%;
                vertical-align: middle;
                margin-right: -4px;
            }

        .modal-dialog {
            display: inline-block;
            text-align: left;
            vertical-align: middle;
        }

        .panel-default > .panel-heading-open {
            background: #E41B17;
        }

        input[type="text"] {
            border: 1px;
            border-style: solid;
            border-color: #006DC0;
            font-family: 'Arial Narrow';
        }

        button[MYTP="BT"] {
            border-top-left-radius:1.5em;
            border-bottom-left-radius:1.5em;
            border-top-right-radius:1.5em;
            border-bottom-right-radius:1.5em;
            height:3em; background-color:#006DC0;color:white;
        }

        textarea.form-control {
            border: 1px;
            border-style: solid;
            border-color: #006DC0;
            font-family: 'Arial Narrow';
            overflow-y: auto !important;
            overflow-x: auto !important;
        }

        select.form-control {
            border: 1px;
            border-style: solid;
            border-color: #006DC0;
            font-family: 'Arial Narrow';
        }


        img[MYTP="BT"]:hover {
            background-color: lightgrey;
        }

        /*thead {
            background-color: #006DC0;
            font-family: 'Arial Narrow';
            color:white;
        }*/

        tbody {
            font-family: 'Arial Narrow';
        }

        table.dataTable tbody td {
            padding: 4px 10px;
        }

        label {
            font-family: 'Arial Narrow';
        }

        div[MYTP="HOVER"]:hover {
            background-color: #ECECEC;
        }

        img[MYTP="HOVER"]:hover {
            background-color: #ECECEC;cursor:pointer;
        }

        div[MYTP="orange"] {
            border-left-width:8px;border-left-style:solid;border-left-color:#FF7400;
        }
        
        div[MYTP="yello"] {
            border-left-width:8px;border-left-style:solid;border-left-color:#FFC000;
        }
        
        div[MYTP="green"] {
            border-left-width:8px;border-left-style:solid;border-left-color:#00B050;
        }
        

        div[MYTP="blue"] {
            border-left-width:8px;border-left-style:solid;border-left-color:#00B0CC;
        }

        div[MYTP="red"] {
            border-left-width:8px;border-left-style:solid;border-left-color:#DF118C;
        }

        input[readonly], select.form-control[readonly], input[disabled], select.form-control[disabled], textarea.form-control[disabled] {
            background-color: #DEEBF7 !important;
        }

        .no-js #loader {
            display: none;
        }

        .js #loader {
            display: block;
            position: absolute;
            left: 100px;
            top: 0;
        }

        .se-pre-con {
            position: fixed;
            left: 0px;
            top: 0px;
            width: 100%;
            height: 100%;
            z-index: 9999;
            background: url('/Content/images/icon/pageloadingicon.gif') center no-repeat transparent !important;
        }

        .borderless td, .borderless th {
                border: none !important;padding:2px !important;
            }
    </style>

</head>

<body>
    <div class="se-pre-con"></div>
    <div class="navbar navbar-default navbar-fixed-top" style="background-color:white;border-color:#5B9BD5">
        <nav class="navbar navbar-default navbar-fixed-top" style="background-color:white;border-color:#5B9BD5">
            <div class="container">

                <div class="row">
                    <div class="col-4">
                        <div class="navbar-header" style="margin-left:20px">
                            <a class="navbar-brand" href="#">
                                <img alt="Brand" src="~/Content/images/FinisarLogo.jpg" style="height:54px; width:334px">
                            </a>
                        </div>
                    </div>
                    <div class="col-lg-8">
                        <div class="row" style="margin-bottom:-8px">
                            <ul class="nav navbar-nav"><li>&nbsp;</li></ul>
                            <ul class="nav navbar-nav navbar-right"><li>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</li></ul>

                        </div>
                        <div class="row" style="margin-top:-8px">
                            <div id="navbar" class="navbar-collapse collapse">
                                <ul class="nav navbar-nav">
                                    <li class="dropdown">
                                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button">MiniPIP<span class="caret"></span></a>
                                        <ul class="dropdown-menu">
                                            <li>
                                                @Html.ActionLink("View All MiniPIP", "ViewAll", "MiniPIP")
                                            </li>
                                            @*<li>
                                                @Html.ActionLink("Create MiniPIP", "CreateMiniPIP", "MiniPIP")
                                            </li>*@
                                        </ul>
                                    </li>
                                </ul>

                                @if (ViewBag.badmin != null && (ViewBag.badmin|| ViewBag.demo))
                                {
                                    <ul class="nav navbar-nav">
                                        <li class="dropdown">
                                            <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" >Report<span class="caret"></span></a>
                                            <ul class="dropdown-menu">
                                                <li>
                                                    @Html.ActionLink("WorkLoad", "WorkLoad", "NebulaReport")
                                                </li>
                                                <li>
                                                    @Html.ActionLink("Cycle Time", "CycleTime", "NebulaReport")
                                                </li>
                                                <li>
                                                    @Html.ActionLink("Type", "Complex", "NebulaReport")
                                                </li>
                                                <li>
                                                    @Html.ActionLink("FA Building Time", "FABuilding", "NebulaReport")
                                                </li>
                                                <li>
                                                    @Html.ActionLink("QA FA Check", "QAFACheck", "NebulaReport")
                                                </li>
                                            </ul>
                                        </li>
                                    </ul>
                                }

                                <ul class="nav navbar-nav">
                                    <li class="dropdown">
                                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">User<span class="caret"></span></a>
                                        <ul class="dropdown-menu">
                                            <li>
                                                @Html.ActionLink("User Center", "UserCenter", "NebulaUser")
                                            </li>
                                            @if (ViewBag.badmin != null && ViewBag.badmin)
                                            {
                                                <li>
                                                    @Html.ActionLink("User Matrix", "CommitUserMatrix", "ExtenalData")
                                                </li>
                                            }
                                        </ul>
                                    </li>
                                </ul>

                                @Html.Partial("_LoginPartial")
                            </div>
                        </div>
                    </div>
                </div>

            </div>
</nav>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - BR Trace System</p>
        </footer>
        <script src="~/Scripts/placeholders.min.js"></script>
        <script type="text/javascript">
            $(window).load(function () {
                $(".se-pre-con").fadeOut("slow");
            });

                        function RealSaveUserContent(myval)
                        {
                            if (myval)
                            {
                                var userdata = {};
                                $.base64.utf8encode = true;
                                userdata['myval'] = $.base64.btoa(myval);

                                $.ajax({
                                    url: "/Nebula/NebulaUser/SaveCacheInfo",
                                    data: userdata,
                                    type: 'POST',
                                    dataType: 'text',
                                    success: function (data, e)
                                    {
                                        alert(data);
                                    },
                                    error: function (e)
                                    {
                                        alert(e);
                                    }
                                });
                            }

                            return false;
                        }

                        function UserCacheContent()
                        {
                            var myurl = '/Nebula/NebulaUser/UserCachedInfo';
                            window.open(myurl, '_blank');
                        }
        </script>

    </div>

    @*@Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")*@
</body>
</html>
